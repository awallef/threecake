{
	"name": "awallef/threecake",
	"description": "a cakephp start point to use with composer and more",
	"minimum-stability": "dev",
	"require": {
		"php": ">=5.2",
		"cakephp/cakephp": "*"
	},
	"autoload": {
		"psr-0": {"": "plugins/Trois/build/composer/"}
	},
	"scripts": {
		"post-install-cmd": ["ThreecakeComposer::afterInstall"]
	},
	"extra": {
		"cakephp-app-dir": "app"
	},
	"repositories": {
		"cakephp-unofficial": {
			"type": "package",
			"package": {
				"name": "cakephp/cakephp",
				"version": "2.3.1",
				"source": {
					"url": "git://github.com/cakephp/cakephp.git",
					"type": "git",
					"reference": "origin/master"
				}
			}
		}
	}
}